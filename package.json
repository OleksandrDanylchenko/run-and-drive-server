{
  "name": "run-and-drive-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": " nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "create-migration": "NODE_ENV=migration ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create PartialUniquenessIndexes",
    "generate-migration": "NODE_ENV=migration ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -p -d src/config/pg-data-source.config.ts src/database/pg/migrations/CarAverageConsumption",
    "migrate": "NODE_ENV=migration npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d src/config/pg-data-source.config.ts",
    "rollback-one": "NODE_ENV=migration ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d src/config/pg-data-source.config.ts"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.4",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.4",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.4",
    "@nestjs/schedule": "^1.1.0",
    "@nestjs/typeorm": "^9.0.0-next.2",
    "argon2": "^0.28.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.0",
    "helmet": "^5.0.2",
    "imgur": "^2.2.0",
    "joi": "^17.6.0",
    "luxon": "^2.3.2",
    "mongodb": "^4.5.0",
    "nanoid": "^3.3.3",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "pg-error-enum": "^0.5.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "spherical-geometry-js": "^3.0.0",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.5",
    "@nestjs/schematics": "^8.0.10",
    "@nestjs/testing": "^8.4.4",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.11",
    "@types/geojson": "^7946.0.8",
    "@types/google.maps": "^3.48.7",
    "@types/jest": "^26.0.22",
    "@types/luxon": "^2.3.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.30",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/../src/$1",
      "^@database/(.*)$": "<rootDir>/../src/database/$1",
      "^@config/(.*)$": "<rootDir>/../src/config/$1",
      "^@common/(.*)$": "<rootDir>/../src/common/$1",
      "^@utils/(.*)$": "<rootDir>/../src/utils/$1",
      "^@auth/(.*)$": "<rootDir>/../src/modules/auth/$1",
      "^@engineers/(.*)$": "<rootDir>/../src/modules/engineers/$1",
      "^@emitters/(.*)$": "<rootDir>/../src/modules/emitters/$1",
      "^@cars/(.*)$": "<rootDir>/../src/modules/cars/$1",
      "^@trips/(.*)$": "<rootDir>/../src/modules/trips/$1",
      "^@test-trips/(.*)$": "<rootDir>/../src/modules/test-trips/$1",
      "^@sensors/(.*)$": "<rootDir>/../src/modules/sensors/$1"
    }
  }
}
